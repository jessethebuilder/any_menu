<div class="container">
  <div class='col-md-4'>
    <h2>Sign in</h2>

    <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
      <div class="form-group">
        <%= f.label :email, :class => 'control-label' %>
        <%= f.email_field :email, :autofocus => true, :class => 'form-control', :value => 'info@any-menu.com' %>
      </div>

      <div class="form-group">
        <%= f.label :password, :class => 'control-label' %>
        <%= f.password_field :password, :class => 'form-control', :auto_complete => 'off', :value => 'testtest' %>
      </div>

      <% if devise_mapping.rememberable? -%>
        <div class="check-box">
          <label class="control-label">
            <%= f.check_box :remember_me %> Remember me?
          </label>
        </div>
      <% end -%>

      <div class='form-group'>
        <%= f.submit "Sign in", :class => 'btn btn-primary form-control' %>
      </div>
    <% end %>

    <div class='form-group'>
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>
